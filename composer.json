{
    "name": "quangbaort/make-repository",
    "description": "Base repository implementation for Laravel",
    "keywords": [
      "laravel",
      "repository",
      "repository pattern",
      "eloquent",
      "repositories"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "Nguyen Quang Bao",
        "email": "quangbaort@gmail.com"
      }
    ],
    "require": {
      "php": ">=7.1",
      "illuminate/support": "~5.1",
      "illuminate/database": "~5.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^4.8",
      "mockery/mockery": "^0.9.4"
    },
    "autoload": {
      "psr-4": {
        "quangbaort\\": "quangbaort/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
            "quangbaort\\RepositoryServiceProvider"
        ]
      }
    },
    "archive" : {
      "exclude": [
        "/test"
      ]
    }
  }
  